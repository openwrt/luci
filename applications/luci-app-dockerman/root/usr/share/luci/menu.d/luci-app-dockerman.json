{
	"admin/services/dockerman": {
		"title": "Dockerman JS",
		"order": "60",
		"action": {
			"type": "firstchild"
		},
		"depends": {
			"acl": [ "luci-app-dockerman" ],
			"fs": {
				"/etc/init.d/dockerd": "executable",
				"/usr/bin/dockerd": "executable"
			},
			"uci": { "dockerd": true }
		}
	},

	"admin/services/dockerman/overview": {
		"title": "Overview",
		"order": 1,
		"action": {
			"type": "view",
			"path": "dockerman/overview"
		}
	},

